
int prime[maxa],minprime[maxa];
int a[maxn],b1[maxn],b2[maxn];
int euler(int n){
    int c=0;
    for(int i=2;i<=n;i++){
        if(!minprime[i]) prime[++c]=i,minprime[i]=i;
        for(int j=1;j<=c&&i*prime[j]<=n;j++){
            minprime[i*prime[j]]=prime[j];
            if(i%prime[j]==0) break;
        }
    }
    return c;
}
